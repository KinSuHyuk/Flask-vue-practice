    "from flask import Flask, jsonify, request\n",
    "import pandas as pd\n",
    "\n",
    "def search():\n",
    "    name = request.args.get('name','')  # name 변수 추출\n",
    "    data = pd.read_excel('소상공인_대구.xlsx')\n",
    "    result = data[data['상호명'] == name].to_dict('records')\n",
    "    return jsonify(result)"
